    "# Interactive tree creation and visualization\n",
    "print(\"=== TREE FUNDAMENTALS ===\\n\")\n",
    "\n",
    "# Create a sample tree for demonstration\n",
    "def create_sample_tree():\n",
    "    \"\"\"Create a sample binary tree for demonstration.\"\"\"\n",
    "    # Level 1\n",
    "    root = TreeNode(1)\n",
    "    \n",
    "    # Level 2\n",
    "    root.left = TreeNode(2)\n",
